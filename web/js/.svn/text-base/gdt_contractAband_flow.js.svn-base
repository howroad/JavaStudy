// 全选
function checkAll(obj){
	smartForm_all_checkbox(obj,'gdt_contractAband_flow_list._check');
}

// 选择某一个
function checkRec(obj){
	nstc.sf.clickCheckbox(obj);
}
function showAbandDetail(obj){
	var fmId=$(obj).parents('tr').first().find('input[id$="fmId"]').val();
	var url="GDEBIT@gdt_conAbondDetail.sf?fmId="+fmId;
	openwin(url,"合同废弃详情",1000,600);
}