function doRefresh(obj){
	var startDate = $('input[id$=.startDate]').val();
	var endDate = $('input[id$=.endDate]').val();
	var startDueDate = $('input[id$=.startDueDate]').val();
	var endDueDate = $('input[id$=.endDueDate]').val();
	if(!compareDate(startDate, endDate, '保函开始日范围不合理')){
		return;
	} 
	if(!compareDate(startDueDate, endDueDate, '保函到期日范围不合理')){
		return;
	} 
	smartForm_doRefresh(obj)
}

function callBack(obj){
	var tr=$(obj).parents('tr').first();
	var result={};
	result.lgmNo=$(obj).html();
	result.billId=tr.find('input[id$="billId"]').val();
	result.contractKind=tr.find("input[id$=.contractKind]").parent().html();
	result.amount=tr.find('input[id$="amount"]').val();
	result.currencyNo=tr.find('input[id$="currencyNo"]').val();
	result.benefitCustName=tr.find('input[id$="benefitCustName"]').val();
	result.financialName=tr.find('input[id$="financialName"]').val();
	result.contractStartDate=tr.find('input[id$="contractStartDate"]').val();
	result.contractEndDate=tr.find('input[id$="contractEndDate"]').val();
	top.Layer.getWin().setGuaranteeInfo(result);
	windowClose();
}