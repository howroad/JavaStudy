function showBussToAll(){
	var url="";
	var controlMethod = $('select[id$=.controlMethod]').val();
	var controlId = $('input[id$=.controlId]').val();
	var year = $('[id$=.year]').val();
	url = "GDEBIT@gdt_limit.sf?m=v&controlMethod="+controlMethod+"&controlId="+controlId+"&saveToAll=1"+"&year="+year;
	openwin(url,'融资业务限额设置',800,700);
		
}
function showBuss(cltNo){
	var url="";
	var controlMethod = $('select[id$=.controlMethod]').val();
	var controlId = $('input[id$=.controlId]').val();
	var year = $('[id$=.year]').val();
	url = "GDEBIT@gdt_limit.sf?m=v&cltNo="+cltNo+"&controlMethod="+controlMethod+"&controlId="+controlId+"&year="+year;
	openwin(url,'融资业务限额设置',800,700);
	
}
