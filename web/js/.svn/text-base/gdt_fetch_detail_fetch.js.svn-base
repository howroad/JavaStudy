$(function(){
	//金额单位
	var unit = $("select[id='gdt_querryFetchRecord_list.moneyUnit']");
	var moneyUnit = $("input[id='gdt_querryFetchRecord_qry.moneyUnit']").val();
	if (moneyUnit !=undefined) {
		 //$(unit).val(moneyUnit);
		$(unit).attr('value',moneyUnit);
	}
	$("span[id$='total']").html($("[id$='selectRecords']").val());
});
function openFetchMoneyRegDetail(obj) {
	var tr = $(obj).parents("tr:first");
	var fetchNo = tr.find("input[id$=fetchNo]").val();
	var contractId = tr.find("input[id$=contractId]").val();
	var fmId = tr.find("input[id$=fmId]").val();
	var url = "GDEBIT@gdt_fetch_detail.sf?m=v&fetchNo=" + fetchNo + "&contractId=" + contractId+"&fmId="+fmId;
	openwin(url, '提款详情', 1200, 1000);
}
//异步加载业务品种
function doChange(obj){
	var bussClass = obj.value;
	$.ajax({
		  type: 'POST',
		  url: "GDEBIT@QueryBussVaresBusiness.sf",
		  async : false,
		  data: {'bussClass':bussClass},
		  dataType:'json',
		  success: function(data) {
			  var bussVar = $("select[id$=.bussVariety]")[0];
			  bussVar.innerHTML = "";
			  if (data.length > 0) {
				  for ( var i = 0; i < data.length; i++) {
					  var name = data[i].name;
					  var value = data[i].value;
					  bussVar[i] = new Option(name, value);
				  }
			  } 
		  }
	})
}
/**
 * 金额单位改变页面
 * @param obj
 */
function setAmountByUnit(obj) {
	var amountUnit = obj.value;
	$("input[id='gdt_querryFetchRecord_qry.moneyUnit']").val(amountUnit);
	var refresh = $("input[id='gdt_querryFetchRecord_qry.refresh']")[0];
	smartForm_doRefresh(refresh);
}
function onclickAmount(obj){
	var tr = $(obj).parent().parent("TR");
	var fetchNo = tr.find("input[id$=fetchNo]").val();
	var url = "GDEBIT@gdt_alreadPayAmount.sf?m=v&fetchNo=" + fetchNo;
	openwin(url, '已还本记录列表', 1200, 1000);
}