//查询
function doQuery(obj){
	var minAmount = $('input[id$=.minAmount]').val();
	var maxAmount = $('input[id$=.maxAmount]').val();

	var startDate = $('input[id$=.minDueDate]').val();
    var endDate = $('input[id$=.maxDueDate]').val();
    if(startDate && endDate && !compareDate(startDate, endDate, '到期日期的起始日期不能大于结束日期')){
        return false;
    }
	if(!compareAmount(minAmount, maxAmount, "发行额的最小金额不能大于最大金额")){
		return;
	} 
	smartForm_doRefresh(obj)
}
