/**
 * 打开单位页面
 */
function openGdtLimit(){
	var check = document.getElementsByName("gdt_nsClientLimit_list._check");
	var cltNoList = '';
	var sum = 0;
	var tr = null;
	for(var i=0;i<check.length;i++){
		if(check[i].checked){
			sum = sum+1;
			tr = nstc.sf.findParent(check[i], 'tr');
			var cltNo = $(tr).find('input[id$=.cltNo]').val();
			cltNoList = cltNoList + cltNo + ',';
		}
	}
	if(sum == 0){
		alert("请选择单位");
		return;
	}
	openwin('GDEBIT@gdt_limit.sf?m=a&cltNoList='+cltNoList,'设置页面', 800, 600);
}


