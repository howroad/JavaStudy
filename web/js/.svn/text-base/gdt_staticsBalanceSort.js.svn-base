function showFinaceContract(obj){
	var tr = nstc.sf.findParent(obj, "TR");
	var currencyNo = $('select[id$=.currencyNo]').val();
	var actDate = $('input[id$=.countDate]').val();
	var cltNo =  $(tr).find('input[id$=_list.cltNo]').val();
	var url = 'GDEBIT@gdt_finaceContract.sf?cltNo=' + cltNo 
	          + '&currencyNo=' + currencyNo
			  + '&actDate=' + actDate
	          + '&balanceFlag=balanceFlag';
	openwin(url, '合同列表', 1000, 700);
}