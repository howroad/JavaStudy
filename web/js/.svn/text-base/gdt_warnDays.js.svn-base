/**
 * 校验是否是正整数
 * @param obj
 */
function checkSave(obj){
	obj.value = obj.value.replace(/[^\d]/g,"");
	if(obj.value==0){
		obj.value = "";
	}
	var reg = /^\+?[1-9][0-9]*$/;
	if (!reg.test(obj.value)){
		alert("请输入正整数");
		obj.value = "";
		obj.focus();
		return ;
	}
}

//单位
function setNsclient(obj){
	$("input[id$='.cltNo']").val(obj.cltNo);
	$("input[id$='.cltName']").val(obj.cltName);
	window.location.href = 'GDEBIT@gdt_warnDays.sf?m=a&cltNo=' 
		+ obj.cltNo + "&cltName=" + obj.cltName;
}