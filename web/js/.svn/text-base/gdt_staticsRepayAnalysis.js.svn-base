/**
 * 年份下拉框设置
 */
$(function(){
	var id = $("select[name$=inspectionyear]").attr("id");
	var temp = $("input[name$=tempinspectionyear]").val();
	var yearObj = document.getElementById(id);
	var date = new Date();
	year = date.getFullYear()-5;
	if(yearObj!=null){
		for(var i=1;i<=10;i++){
			var optionObj=document.createElement("option");
			optionObj.value=year+i;
			optionObj.innerHTML=year+i;
			yearObj.appendChild(optionObj);
			if(temp!=null){
				if(optionObj.value == temp){
					optionObj.selected = "selected";
				}
			}else {
				if(optionObj.value == date.getFullYear()){
					optionObj.selected = "selected";
				}
			}
		}
	}
});