//间接融资-贸易融资》保理	
$(function(obj){
	/*var bussClass = $("select[id$=.bussClass]").val('CLMS03');*/
	var bussClass = $("select[id$='.bussClass']").attr('value','CLMS03');
	doChange(bussClass[0]);
	//隐藏业务品种和分类,展示值
		/*$("select[id$=.bussVariety]").val('CLMS23');*/
	$("select[id$='.bussVariety']").attr('value','CLMS23');
	var bussVariety = $("select[id$=.bussVariety]");
	var cltName = $("input[id$=.cltName]").val();
	var apply = $("input[id$='_form.applyId']").val();

	if(apply != undefined && apply != null && apply != ''){
		var clt = $("input[id$=_info.cltName]");
		doHiddenApply(clt);
		doHidden(bussClass,bussVariety);
	}
	
	var recourse = $("input[id$='_info.isRecourse']").val();
	if(recourse == "0"){
		$("input[id$='recourse']").attr('checked', 'checked');
	}
	
	var m = document.getElementById('m').value;
	if(m!=null && m=='v'){
		$("[id$='_info.recourse']:checkbox").attr("disabled",true);
	}
	if(m=='e' || m == 'v' || m == ''){
		doHidden(bussClass,bussVariety);
	}
	
	var channel = $("input[id$='gdt_conFactor_form.channel']").val();
    //当channel字段不为GDEBIT时，将带入的数据置换为不可写
	if(isNotNull(channel) && channel != "GDEBIT"){
		var table = $("#gdt_conFactor_info");
		doReadOnly(table);
	}
});


function checkIsRecourse(){
	var recourse = $("input[id$='_info.isRecourse']").val();
	if(recourse == null || recourse == "" || recourse == "1"){
		$("input[id$='_info.isRecourse']").val("0");
	}else{
		$("input[id$='_info.isRecourse']").val("1");
	}
}