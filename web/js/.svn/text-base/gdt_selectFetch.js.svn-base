function selParPlanNo(obj) {
	var tr = $(obj).parent().parent('TR');
	var contexecAmount = $(tr).find('input[id$="contexecAmount"]').val();
	var fetchAmount = $(tr).find('input[id$="planAmount"]').val();
	var startDate = $(tr).find('input[id$="startDate"]').val();
	var endDate = $(tr).find('input[id$="endDate"]').val();
	var feesVarietyNo = $(tr).find('input[id$="feesVarietyNo"]').val();
	var fetchNo = $(tr).find('input[id$="fetchNo"]').val();
	var actDate = $(tr).find('input[id$="actDate"]').val();
	var assBankName = $(tr).find('input[id$="assBankName"]').val();
	var assBankId = $(tr).find('input[id$="assBankId"]').val();
	var rate = $(tr).find('input[id$="rate"]').val(); 
	var currencyNo = $(tr).find('input[id$="currencyNo"]').val();
	var planType = $(tr).find('input[id$="planType"]').val();
	var guaranteeId = $(tr).find('input[id$="guaranteeId"]').val();
	var guaranteePlanId = $(tr).find('input[id$="guaranteePlanId"]').val();
	
	var data = {};
	data.startDate=startDate;
	data.endDate=endDate;
	data.feesVarietyNo=feesVarietyNo;
	data.fetchNo=fetchNo;
	data.currencyNo=currencyNo;
	data.actDate=actDate;
	data.assBankName=assBankName;
	data.assBankId=assBankId;
	data.rate=rate;
	data.planType=planType;
	data.fetchAmount=fetchAmount;
	data.associateNo=obj.innerHTML;
	data.guaranteeId=guaranteeId;
	data.guaranteePlanId=guaranteePlanId;
	top.Layer.getWin().setParPlanNo(data);
	windowClose();
}