function callBack(obj){
	var tr = nstc.sf.findParent(obj, "TR");
	var assBankName = $(tr).find("input[id$='.assBankName']").val();
	var assBankId = $(tr).find("input[id$='.assBankId']").val();
	var bankNo = $(tr).find("input[id$='.bankNo']").val();
	var bankName = $(tr).find("input[id$='.bankName']").val();
	var rowIndex = $("input[id$='.rowIndex']").val();
	var flag = $("input[id$='.flag']").val();
	if(assBankId=='Çå¿ÕÊý¾Ý'){
		assBankId = "";
		assBankName = "";
	}
	var result = {};
	result.assBankId = assBankId;
	result.assBankName = assBankName;
	result.bankNo = bankNo;
	result.bankName = bankName;
	result.rowIndex = rowIndex;
	result.flag = flag;
	top.Layer.getWin().setAssBank(result);
	windowClose();
}