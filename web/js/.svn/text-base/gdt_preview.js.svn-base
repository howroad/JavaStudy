$(function() {
	var date = new Date();// 默认为当天
	var year = date.getFullYear();
	var month = date.getMonth() + 1;
	var day = date.getDate();
	if (month < 10) {
		month = "0" + month;
	}
	if (day < 10) {
		day = "0" + day;
	}
	var start = $("[id$=gdt_qrypreview_qry.startDate]").val();
	if (isNull(start)) {
		$("[id$=gdt_qrypreview_qry.startDate]").val(year + "-" + month + "-" + day);
	}
	var end = $("[id$=gdt_qrypreview_qry.endDate]").val();
	if (isNull(end)) {
		$("[id$=gdt_qrypreview_qry.endDate]").val(year + "-" + month + "-" + day);
	}
});

function doRefresh(obj){
	var start = $("[id$=gdt_qrypreview_qry.startDate]").val();
	var end = $("[id$=gdt_qrypreview_qry.endDate]").val();
	//alert(withHoldDate);
	if(isNull(start)||isNull(end)){
		alert("预提日期不能为空");
	}else{
		smartForm_doRefresh(obj);
	}
} 

//判断是否为空
function isNull(a){
	  if(a == "" || a == null || a == undefined){ // "",null,undefined
	       return true;
	    }
	  return false;
}