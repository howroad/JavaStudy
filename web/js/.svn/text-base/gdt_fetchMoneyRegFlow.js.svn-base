$(function (){
	//动态显示授信品种
	var bussClass = $("select[id$='_con.bussClass']").get(0);
	var bussVariety = $("select[id$='_con.bussVariety']");
	var bussVarietyValue = bussVariety.val();
	queryBussVares(bussClass, '');
	var bussVarietyArr = $("select[id$='_con.bussVariety'] option");
	if (bussVarietyArr.eq(0).val() == '') {
		bussVarietyArr.eq(0).remove();
	}
	if (bussVarietyValue != '') {
		bussVariety.val(bussVarietyValue);
	}
	
	//隐藏：维护还款计划
	$("a[id='gdt_ledgerPlan_drawings.el_14']").each(function(){
		var tr = $(this).parents("tr").first();
		var fetchNo = tr.find("input[id$='.fetchNo']").val();
		if (fetchNo == '') {
			$(this).hide();
		}
	});
	
	//如果合同信息不为空，则合同信息不可以修改
	var contractId = $("input[id$='_con.contractId']").val();
	if (contractId != '' && contractId != null) {
		$("select[id$='_con.bussClass']").attr('disabled','true');
		$("select[id$='_con.bussVariety']").attr('disabled','true');
	}
	
	//如果是修改界面，则合同编号不能再次修改
	var fmId = $("input[id$='fmId']").val();
	if (fmId != '' && fmId != null) {
		$("input[id$='_con.contractNo']").attr('disabled','true');
	}
});
function showFetchMoneyDetail(fmId, contractId, obj) {
	var url = "GDEBIT@gdt_addFetchMoneyReg.sf?m=v&fmId=" + fmId + "&contractId=" + contractId;
	openwin(url, '提款到账详情', 1200, 1000);
}