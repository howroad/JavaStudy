/**
 * 台账管理录入界面
 * @param obj
 * @param type//1，台账计划；2，发生额登记；3，资金使用登记；
 */
function showLedger(obj,type,m){
	var tr=$(obj).parents('tr').first();
	var contractId=tr.find('input[id$="contractId"]').val();
	var bussVariety=tr.find('input[id$="bussVariety"]').val();
	var contexecId=obj.innerHTML;
	var url='';
	if(bussVariety==undefined){
		return;
	}else if(bussVariety=='CLMS01'){
		/**股票*/
		url="GDEBIT@gdt_ledgerStock.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS02'){
		/**债券*/
		url="GDEBIT@gdt_ledgerBond.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS03'){
		/**信托产品*/
		url="GDEBIT@gdt_ledgerTrust.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS04'){
		/**其他直接融资*/
		url="GDEBIT@gdt_ledgerDirOther.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS05'){
		/**流动资金贷款*/
		url="GDEBIT@gdt_ledgerLiquidity.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS06'){
		/**项目贷款*/
		url="GDEBIT@gdt_ledgerProject.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS07'){
		/**银团贷款*/
		url="GDEBIT@gdt_ledgerSyndicate.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS08'){
		/**委托贷款*/
		url="GDEBIT@gdt_ledgerEntrust.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS09'){
		/**固定资产贷款*/
		url="GDEBIT@gdt_ledgerFixed.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS10'){
		/**承兑汇票*/
		url="GDEBIT@gdt_ledgerAccbill.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS11'){
		/**票据贴现*/
		url="GDEBIT@gdt_ledgerBillDisc.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS12'){
		/**内保外贷*/
		url="GDEBIT@gdt_ledgerInOut.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS13'){
		/**内存外贷*/
		url="GDEBIT@gdt_ledgerOutOfMemory.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS14'){
		/**其他贷款*/
		url="GDEBIT@gdt_ledgerLoanOther.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS15'){
		/**进口L/C开证*/
		url="GDEBIT@gdt_ledgerImportLC.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS16'){
		/**进口T/T押汇*/
		url="GDEBIT@gdt_ledgerImportTBill.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS17'){
		/**进口L/C押汇*/
		url="GDEBIT@gdt_ledgerImportLCBill.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS18'){
		/**T/T代付*/
		url="GDEBIT@gdt_ledgerTPayForAn.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS19'){
		/**L/C代付*/
		url="GDEBIT@gdt_ledgerLCPayForAn.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS20'){
		/**出口T/T押汇*/
		url="GDEBIT@gdt_ledgerExportTBill.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS21'){
		/**出口L/C押汇*/
		url="GDEBIT@gdt_ledgerExportLCBill.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS22'){
		/**出口L/C打包贷款*/
		url="GDEBIT@gdt_ledgerExportLCLoan.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS23'){
		/**保理*/
		url="GDEBIT@gdt_ledgerFactor.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS24'){
		/**保函*/
		url="GDEBIT@gdt_ledgerGuarantee.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS25'){
		/**其它贸易融资*/
		url="GDEBIT@gdt_ledgerIndOther.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}else if(bussVariety=='CLMS26'){
		/**融资租赁*/
		url="GDEBIT@gdt_ledgerFinLease.sf?m="+m+"&type="+type+'&contractId='+contractId+'&contexecId='+contexecId;
	}
	if(url!=""){
		if(type==1){
			openwin(url, '台账计划', 1200,800);
		}else if(type==2){
			openwin(url, '发生额登记', 1200,800);
		}else if(type==3){
			openwin(url, '资金使用登记', 1200,800);
		}
	}
}