$(function(){
	var queryCltNo = $("[id$=queryCltNo]").val();
	var queryCltNos = queryCltNo.split(",");
	var cltNos = $("[id$='gdt_queryStatisticsCltNo_list.cltNo']");
	if (queryCltNos.length > 0 && cltNos.length > 0) {
		for (var i = 0; i < cltNos.length; i++) {
			var cltNo = $(cltNos[i]).val();
			for (var m = 0; m < queryCltNos.length; m++) {
				var checkCltNo = queryCltNos[m];
				if (cltNo == checkCltNo) {
					$(cltNos[i]).closest("tr").find("[id$='_check']").attr("checked","checked");
					break;
				}
			}
		}
	}
});